{
  "name": "love-lock-supabase",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npx supabase start\" \"npm run start:frontend\"",
    "start:frontend": "cd frontend && npm install && npm start",
    "build": "npm run build:frontend",
