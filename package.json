    "migrate:status": "npx supabase db status",
    "supabase:login": "npx supabase login"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "supabase": ">=1.8.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-select": "^5.10.1"
  }
}
